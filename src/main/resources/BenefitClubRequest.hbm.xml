<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dk.developer.delta.api.concepts.BenefitClubRequest" table="AppUserBenefitClubRequestView">
        <id name="_id" column="UserProgramRequestID" type="string">
            <generator class="native"/>
        </id>

        <property name="userId" column="UserID" type="string"/>
        <property name="benefitClubName" column="ProgramName" type="string"/>
        <property name="benefitClubOrganisation" column="ProgramOwnerName" type="string"/>
        <property name="benefitClubDescription" column="ProgramDescription" type="string"/>
    </class>
</hibernate-mapping>