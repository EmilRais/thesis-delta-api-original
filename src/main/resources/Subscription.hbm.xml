<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dk.developer.delta.api.concepts.Subscription" table="AppSubscriptionView">
        <id name="_id" column="ID" type="string">
            <generator class="native"/>
        </id>

        <property name="userId" column="UserID" type="string"/>
        <property name="benefitClubId" column="ProgramID" type="string"/>
        <property name="membershipNumber" column="LocalMembershipID" type="string"/>
        <property name="membershipLevel" column="MembershipLevelID" type="string"/>
        <property name="date" column="CreateDBTime" type="string"/>
    </class>
</hibernate-mapping>