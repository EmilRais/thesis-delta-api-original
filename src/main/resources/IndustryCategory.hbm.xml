<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dk.developer.delta.api.concepts.IndustryCategory" table="AppCategoryIndustryView">
        <id name="_id" column="IndustryID" type="string">
            <generator class="native"/>
        </id>

        <property name="name" column="IndustryName" type="string"/>
    </class>
</hibernate-mapping>